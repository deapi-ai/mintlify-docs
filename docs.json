{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "deAPI",
  "logo": {
    "light": "/logo/deapi-light.svg",
    "dark": "/logo/deapi-dark.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://deapi.ai/dashboard"
      },
      {
        "label": "Status",
        "href": "https://status.deapi.ai/"
      },
      {
        "label": "GitHub",
        "href": "https://github.com/deapi-ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://deapi.ai/dashboard"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/deapi-ai",
      "twitter": "https://x.com/deAPI_",
      "discord": "https://discord.com/invite/UFfK5YRBsr"
    },
    "links": [
      {
        "title": "Product",
        "items": [
          {
            "label": "Pricing",
            "href": "https://deapi.ai/pricing"
          },
          {
            "label": "Dashboard",
            "href": "https://deapi.ai/dashboard"
          },
          {
            "label": "Status",
            "href": "https://status.deapi.ai/"
          }
        ]
      },
      {
        "title": "Resources",
        "items": [
          {
            "label": "API Reference",
            "href": "/api-reference/text-to-image"
          },
          {
            "label": "Quickstart",
            "href": "/quickstart"
          },
          {
            "label": "FAQ",
            "href": "/resources/faq"
          }
        ]
      }
    ]
  },
  "seo": {
    "indexHiddenPages": false
  },
  "search": {
    "prompt": "Search deAPI docs..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "api": {
    "baseUrl": "https://api.deapi.ai",
    "playground": {
      "mode": "simple"
    }
  },
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "introduction",
          "quickstart",
          "models",
          "pricing"
        ]
      },
      {
        "group": "Platform",
        "pages": [
          "platform/architecture-security",
          "platform/limits-quotas",
          {
            "group": "Execution Modes",
            "icon": "bolt",
            "pages": [
              "platform/execution-modes/http-queue",
              "platform/execution-modes/mcp-server"
            ]
          }
        ]
      },
      {
        "group": "API Reference",
        "icon": "code",
        "pages": [
          {
            "group": "Image Generation",
            "icon": "image",
            "pages": [
              "api-reference/text-to-image",
              "api-reference/image-to-image",
              "api-reference/image-background-removal"
            ]
          },
          {
            "group": "Video Generation",
            "icon": "video",
            "pages": [
              "api-reference/text-to-video",
              "api-reference/image-to-video"
            ]
          },
          {
            "group": "Audio & Speech",
            "icon": "microphone",
            "pages": [
              "api-reference/text-to-speech",
              "api-reference/upload-audio-file"
            ]
          },
          {
            "group": "Vision & OCR",
            "icon": "eye",
            "pages": [
              "api-reference/image-to-text",
              "api-reference/video-to-text",
              "api-reference/upload-video-file"
            ]
          },
          {
            "group": "Embeddings",
            "icon": "brain",
            "pages": [
              "api-reference/text-to-embedding"
            ]
          },
          {
            "group": "Account & Results",
            "icon": "user",
            "pages": [
              "api-reference/check-balance",
              "api-reference/get-results",
              "api-reference/model-selection"
            ]
          }
        ]
      },
      {
        "group": "Resources",
        "pages": [
          "resources/sdks-integrations",
          "resources/payment-methods",
          "resources/faq",
          "resources/support"
        ]
      }
    ]
  },
  "anchors": [
    {
      "anchor": "API Reference",
      "icon": "code",
      "href": "/api-reference/text-to-image"
    },
    {
      "anchor": "Status",
      "icon": "signal",
      "href": "https://status.deapi.ai/"
    },
    {
      "anchor": "Discord",
      "icon": "discord",
      "href": "https://discord.com/invite/UFfK5YRBsr"
    }
  ],
  "mcp": {
    "enabled": true
  }
}
